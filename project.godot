[android]

modules="org/godotengine/godot/Flashlight"

[application]

config/name="YouAreUto"
run/main_scene="res://scenes/Main.tscn"
boot_splash/image="res://assets/sprites/splash-img.png"
boot_splash/bg_color=Color( 0.941176, 0.941176, 0.941176, 1 )
config/icon="res://icon.png"

[autoload]

Global="*res://autoloads/Global.gd"
SceneManager="*res://autoloads/SceneManager.gd"
Layout="*res://addons/layouts/layout_manager.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=750
window/size/height=1334
window/size/test_width=375
window/size/test_height=667
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/layouts/plugin.cfg" )

[importer_defaults]

ogg_vorbis={
"loop": false,
"loop_offset": 0
}

